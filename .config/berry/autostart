 #!/bin/bash

#===========
# Autostart
#===========
$HOME/.config/rofi/config
$HOME/.config/dunst/dunstrc
$HOME/.config/polybar/launch.sh

#picom
#$HOME/picom.conf

# Set decoration geometry
berryc border_width       5
berryc inner_border_width 3
berryc title_height       30
berryc edge_gap           55 10 10 10


# Set decoration colors
berryc focus_color         20292D
berryc unfocus_color       20292D
berryc inner_focus_color   607A86
berryc inner_unfocus_color 2C3539
berryc text_focus_color    eeeeee
berryc text_unfocus_color  eeeeee

# Other options
berryc smart_place "true"
berryc draw_text   "true"
berryc edge_lock   "true"
berryc set_font    "Dina-10"

sxhkd -c $HOME/.config/berry/sxhkdrc &

#blurwal
blurwal --m 1

#feh wallpaper
#$HOME/.fehbg

firefox
dunst

xsetroot -cursor_name left_ptr
pamac-tray
/home/babyjesus/.config/autostart/Power Manager
numlockx on
/usr/lib/xfce4/notifyd/xfce4-notifyd
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1
